{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { 
      "src": "/", 
      "dest": "/server.js" 
    },
    { 
      "src": "/en", 
      "dest": "/server.js" 
    },
    { 
      "src": "/hi", 
      "dest": "/server.js" 
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    { 
      "src": "/(.*)", 
      "dest": "/server.js" 
    }
  ]
}